<Window x:Class="TP2_POO2.View.FichierCSV"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TP2_POO2.View"
        mc:Ignorable="d"
        Title="FichierCSV" Height="450" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <ScrollViewer>
            <TextBlock Name="Donnees" Text="" Grid.Column="0"></TextBlock>
        </ScrollViewer>
        <Button Content="Quitter" Grid.Column="1" Width="200" Height="40" Click="Quitter"></Button>
    </Grid>
</Window>
